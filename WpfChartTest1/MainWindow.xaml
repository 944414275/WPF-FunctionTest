<Window x:Class="WpfChartTest1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfChartTest1"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <GroupBox>
            <d3:ChartPlotter x:Name="plotter">
                <d3:Header TextBlock.FontSize="20" >
                    Demo
                </d3:Header>
                <d3:VerticalAxisTitle>yAxies</d3:VerticalAxisTitle>
                <d3:HorizontalAxisTitle>xAxies</d3:HorizontalAxisTitle>
            </d3:ChartPlotter>
        </GroupBox> 
    </Grid>
</Window>
